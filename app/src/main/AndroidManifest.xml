<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.apartamentos.moviles2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Apartamentosapp">
        <activity android:name=".EditarApartamento"></activity>
        <activity android:name=".Apartamentos" />
        <activity android:name=".RegistroApartamentos" />
        <activity android:name=".Usuarios" />
        <activity android:name=".ActualizarUsuario" />
        <activity android:name=".Registrousuario" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>